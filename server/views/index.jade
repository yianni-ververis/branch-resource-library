      '//www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
      })(window,document,'script','dataLayer','GTM-P7VJSX');  
    confirm-dialog
    div(header)
    div.main(ui-view)
    div(footer)
